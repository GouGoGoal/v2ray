{ 
  "api": {
    "services": [
      "HandlerService",
      "LoggerService",
      "StatsService",
      "RuleService"
    ],
    "tag": "api"
  },
  "inbounds": [{
    "listen": "127.0.0.1",
    "port": 2000,
    "protocol": "dokodemo-door",
    "settings": {
      "address": "127.0.0.1"
    },
    "tag": "api"
  }
  ],
  "log_bak": {
    "loglevel": "info"
  },
  "outbounds": [{
    "protocol": "freedom",
    "settings": {
      "domainStrategy": "UseIP"
    }
  },
    {
      "protocol": "blackhole",
      "settings": {"response": {
        "type": "http"
      }},
      "tag": "blocked"
    }, {
      "protocol": "blackhole",
      "settings": {"response": {
        "type": "http"
      }},
      "tag": "blockedrule"
    },{
      "protocol": "blackhole",
      "settings": {"response": {
        "type": "http"
      }},
      "tag": "blockedip"
    },{
      "protocol": "blackhole",
      "settings": {"response": {
        "type": "none"
      }},
      "tag": "blockedNodeUserLimited"
    },{
        "protocol": "blackhole",
        "settings": {},
        "tag": "block"
      }
  ],
  "policy": {
    "levels": {
      "0": {
        "connIdle": 300,
        "downlinkOnly": 0,
        "handshake": 4,
        "statsUserDownlink": true,
        "statsUserUplink": true,
        "uplinkOnly": 0
      }
    },
    "system": {
      "statsInboundDownlink": false,
      "statsInboundUplink": false
    }
  },
  "reverse": {},
  "routing": {
    "rules": [{
      "outboundTag": "blocked",
      "protocol": [
        "bittorrent"
      ],
      "type": "field"
    },
      {
        "inboundTag": [
          "api"
        ],
        "outboundTag": "api",
        "type": "field"
      },{
        "outboundTag": "blockedNodeUserLimited",
        "user": [
          "admin@soulout.club"
        ],
        "type": "field"
      }
    ],
    "domainStrategy": "AsIs"
  },
  "stats": {},
  "sspanel": {
    "nodeid": id_value,
    "checkRate": 60,
    "SpeedTestCheckRate": 0,
    "panelUrl": "https://api.soulout.club",
    "panelKey": "souloutclub",
    "downWithPanel": 1,
    "mu_regex":  "%5m%id.%suffix",
    "mu_suffix": "microsoft.com",
    "mysql": {
      "host": "3306.soulout.club",
      "port": 3306,
      "user": "souloutclub@souloutclub",
      "password": "souloutclub",
      "dbname": "panel"
    },
    "paneltype": 0,
    "usemysql": 0,
    "proxy_tcp": 0
  }
}

